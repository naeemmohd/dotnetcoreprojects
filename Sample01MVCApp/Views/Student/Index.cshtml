@model Sample01MVCApp.Models.Student
<html>
<head>
    <title>Student Details</title>
</head>
<body>
    <br />
    <h1>AppName: @TempData["AppName"]</h1>
    <br />
    <table>
        <tr>
            <td>Student ID: </td>
            <td>@Model.StudentId</td>
        </tr>
        <tr>
            <td>Name: </td>
            <td>@Model.Name</td>
        </tr>
        <tr>
            <td>Gender: </td>
            <td>@Model.Gender </td>
        </tr>
        <tr>
            <td>Branch: </td>
            <td>@Model.Branch</td>
        </tr>
        <tr>
            <td>Section: </td>
            <td>@Model.Section </td>
        </tr>
    </table>
</body>
</html>